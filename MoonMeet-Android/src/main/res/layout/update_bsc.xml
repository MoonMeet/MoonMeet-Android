<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/bg1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="17dp"
		android:padding="17dp"
		android:background="#FFFFFF"
		android:orientation="horizontal">
		<LinearLayout
			android:id="@+id/linear10"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/card"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/i1"
					android:focusable="false"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:src="@drawable/logo"
					android:scaleType="centerInside" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear12"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_marginLeft="17dp"
			android:gravity="left|center_vertical"
			android:orientation="vertical">
			<TextView
				android:id="@+id/t3"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginBottom="3dp"
				android:text="Moon Meet"
				android:textSize="16sp"
				android:textColor="#193566" />
			<TextView
				android:id="@+id/t4"
				android:focusable="false"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Version 87.1.1"
				android:textSize="14sp"
				android:textColor="#9E9E9E" />
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/bg2"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="17dp"
		android:layout_marginRight="17dp"
		android:layout_marginBottom="17dp"
		android:padding="17dp"
		android:background="#FFFFFF"
		android:orientation="vertical">
		<TextView
			android:id="@+id/t1"
			android:focusable="false"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="7dp"
			android:gravity="left|center_vertical"
			android:text="What's new \?"
			android:textSize="16sp"
			android:textColor="#212121"
			android:alpha="0.8" />
		<TextView
			android:id="@+id/t2"
			android:focusable="false"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="17dp"
			android:gravity="left|center_vertical"
			android:text="- Improvement on Profile page UI/UX\n- Brand new like &amp; appreciations for posts boys\n- Now users can access to posts photos and download them\n- Now comments support GIFs and stickers provide by GitHub."
			android:textSize="14sp"
			android:textColor="#9E9E9E" />
		<TextView
			android:id="@+id/b2"
			android:focusable="false"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="17dp"
			android:padding="12dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="Remind me"
			android:textSize="14sp"
			android:textColor="#616161"
			android:visibility="invisible" />
		<TextView
			android:id="@+id/b1"
			android:focusable="false"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="17dp"
			android:padding="12dp"
			android:background="#193566"
			android:gravity="center_horizontal|center_vertical"
			android:text="Install now"
			android:textSize="14sp"
			android:textColor="#FFFFFF" />
	</LinearLayout>
</LinearLayout>
